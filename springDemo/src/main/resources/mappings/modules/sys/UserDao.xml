<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jimmieRan.springDemo.modules.sys.dao.UserDao">


    <select id="findByUsername" parameterType="String" resultType="com.jimmieRan.springDemo.modules.sys.entity.User">
        SELECT * FROM sys_user u WHERE u.username=#{username}
    </select>

</mapper>